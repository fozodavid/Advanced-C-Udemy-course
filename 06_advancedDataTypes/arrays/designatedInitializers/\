#include <stdio.h>

int main() {
  /*
  int a[6] = {[4] = 29, [2] = 15};
  int b[6] = {[4]25, [2]15};

  int c[5] = { 0, 0, 15, 0, 29, 0};
  */

  int numbers[100] = {1,2,3,[3 ... 9] = 10, [10] = 80, 15, [70]=50, [42] = 42};

  for (int i = 0; i < 20; i++) {
    printf("%d ", numbers[i]);
  }
  printf("\n%d ", numbers[70]);
  printf("%d", numbers[42]);
  return 0;
}
